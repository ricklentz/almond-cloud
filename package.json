  "scripts": {
    "update-pot": "xgettext -f po/POTFILES -o po/${npm_package_name}.pot --from-code UTF-8 --package-name ${npm_package_name} --package-version ${npm_package_version}; xargs -a po/POTFILES.jade jsxgettext -k _,gettext,ngettext,pgettext -L jade -o po/${npm_package_name}.pot -j",
    "update-po": "for i in po/*.po ; do msgmerge -U $i po/${npm_package_name}.pot ; done",
    "compile-mo": "for i in po/*.po ; do msgfmt $i -o po/`basename $i .po`.mo ; done",
    "update-bundle": "./scripts/update-bundles.sh",
    "update-docs": "./scripts/update-docs.sh",
    "lint": "eslint almond/ browser/ model/ routes/ util/ stanford/ training/ config.js main.js frontend.js",
    "test": "nyc ./tests/everything.sh",
    "prepublish": "which yarn >/dev/null 2>&1 && ( yarn compile-mo && yarn update-docs && yarn update-bundle ) || ( npm run compile-mo && npm run update-docs && npm run update-bundle )",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "install": "make -C sandbox all || echo WARNING: failed to compile the sandbox"
  },
  "resolutions": {
    "highlight.js": "github:Stanford-Mobisocial-IoT-Lab/highlight.js",
    "thingtalk": "git://github.com/Stanford-Mobisocial-IoT-Lab/thingtalk"
  },
